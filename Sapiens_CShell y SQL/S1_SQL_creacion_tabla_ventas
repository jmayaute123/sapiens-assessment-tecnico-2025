CREATE OR REPLACE TABLE `sapiens-assessment-pipiline.sapiens_data.ventas`
(
  id_venta STRING,
  fecha_venta DATE,
  id_cliente STRING,
  id_producto STRING,
  cantidad INT64,
  precio_unitario FLOAT64,
  costo_unitario FLOAT64, -- **NUEVO CAMPO** para calcular margen
  descuento FLOAT64,
  canal_venta STRING
)
PARTITION BY fecha_venta
CLUSTER BY id_producto, id_cliente;